$jason: {
  head: {
    title: hi
    actions: {
      $load: {
        type: $network.request
        options: {
          url: https://....
        }
        success: {
          type: $render
        }
      }
    }
    templates: {
      body: {
        sections: [{
          items: {
            "{{#each $jason}}" : {
              type: "horizontal"
              components: [
                {
                  type: "image"
                  url: "{{image}}"
                }
                {
                  type: "vertical"
                  components: [
                    {
                      type: "label"
                      text: "{{name}}"
                    }
                    {
                      type: "label"
                      text: "{{status}}"
                    }
                  ]
                }
              ]
            }
          }
        }]
      }
    }
  }
}
